{
    "compilerOptions": {
      "module": "Node16",
      "lib": [
        "es6"
      ],
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "allowJs": true,
      "moduleResolution": "node",
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true,
      "noUnusedLocals": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    }
  }